@article{sichova2022,
  title = {Pharmacokinetics, Systemic Toxicity, Thermoregulation and Acute Behavioural Effects of {{25CN-NBOMe}}},
  author = {Šíchová, Klára and Syrová, Kateřina and Kofroňová, Edita and Pinterova-Leca, Nikola and Vejmola, Čestmír and Nykodemová, Jitka and Palivec, Petr and Olejníková, Lucie and Danda, Hynek and Jorratt, Pascal and Adam, Šafanda and Hiep, Bui Quang and Štefková-Mazochová, Kristýna and Končická, Markéta and Kuchař, Martin and Páleníček, Tomáš},
