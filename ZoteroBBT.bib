@article{sadeghiyeh2020,
  title = {Temporal Discounting Correlates with Directed Exploration but Not with Random Exploration},
  author = {Sadeghiyeh, Hashem and Wang, Siyu and Alberhasky, Maxwell R. and Kyllo, Hannah M. and Shenhav, Amitai and Wilson, Robert C.},
  date = {2020-03-04},
